<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<FlowPane alignment="CENTER" columnHalignment="CENTER"
	orientation="VERTICAL" vgap="20.0" xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="nl.mvdr.tinustris.controller.InputConfigurationController">
	<children>
		<Label fx:id="buttonPromptLabel" text="Please press the button for &lt;input&gt;.">
			<font>
				<Font size="16.0" />
			</font>
		</Label>
		<Label fx:id="descriptionLabel" text="&lt;Description&gt;">
			<font>
				<Font size="16.0" />
			</font>
		</Label>
		<Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancel"
			text="Cancel" focusTraversable="false" />
	</children>
</FlowPane>
